                        android:textSize="15sp"
                        android:fontFamily="@font/roboto_regular"/>


                </LinearLayout>


                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="60dp"
                    android:orientation="horizontal"
                    android:background="@drawable/shadow"
